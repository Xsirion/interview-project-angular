{
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "es5",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "avoid",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "lf",
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular",
          "printWidth": 120
        }
      },
      {
        "files": "*.component.html",
        "options": {
          "parser": "angular",
          "printWidth": 120
        }
      }
    ]
  }